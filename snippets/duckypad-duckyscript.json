{
    "Comment": {
        "prefix": "REM",
        "body": [
            "REM ${1:Comment}"
        ],
        "description": "BASIC/Win style comment"
    },
    "Comment Block": {
        "prefix": "REM_BLOCK",
        "body": [
            "REM_BLOCK",
            "\t$0",
            "END_REM"
        ],
        "description": "Block comment"
    },
    "String": {
        "prefix": "STRING",
        "body": [
            "STRING ${1:text}"
        ],
        "description": "Types out text as-is"
    },
    "String Line": {
        "prefix": "STRINGLN",
        "body": [
            "STRINGLN ${1:text}"
        ],
        "description": "Types text and presses Enter at the end"
    },
    "String Block": {
        "prefix": "STRING_BLOCK",
        "body": [
            "STRING_BLOCK",
            "\t$0",
            "END_STRING"
        ],
        "description": "Types everything inside as-is (no Enter on new lines)"
    },
    "String Line Block": {
        "prefix": "STRINGLN_BLOCK",
        "body": [
            "STRINGLN_BLOCK",
            "\t$0",
            "END_STRINGLN"
        ],
        "description": "Types everything inside, press Enter at end of each line"
    },
    "Delay": {
        "prefix": "DELAY",
        "body": [
            "DELAY ${1:milliseconds}"
        ],
        "description": "Pause execution for n milliseconds"
    },
    "Default Delay": {
        "prefix": "DEFAULTDELAY",
        "body": [
            "DEFAULTDELAY ${1:20}"
        ],
        "description": "Wait time between non-letter input actions"
    },
    "Default Char Delay": {
        "prefix": "DEFAULTCHARDELAY",
        "body": [
            "DEFAULTCHARDELAY ${1:20}"
        ],
        "description": "Wait time between each letter when typing"
    },
    "Char Jitter": {
        "prefix": "CHARJITTER",
        "body": [
            "CHARJITTER ${1:0}"
        ],
        "description": "Adds random delay up to n milliseconds after each letter"
    },
    "Repeat": {
        "prefix": "REPEAT",
        "body": [
            "REPEAT ${1:times}"
        ],
        "description": "Repeats the last line n times"
    },
    "Key Down": {
        "prefix": "KEYDOWN",
        "body": [
            "KEYDOWN ${1:KEY_NAME}"
        ],
        "description": "Hold a key down"
    },
    "Key Up": {
        "prefix": "KEYUP",
        "body": [
            "KEYUP ${1:KEY_NAME}"
        ],
        "description": "Release a key"
    },
    "Mouse Move": {
        "prefix": "MOUSE_MOVE",
        "body": [
            "MOUSE_MOVE ${1:x} ${2:y}"
        ],
        "description": "Move mouse cursor (X: +/- Right/Left, Y: +/- Up/Down)"
    },
    "Mouse Scroll": {
        "prefix": "MOUSE_SCROLL",
        "body": [
            "MOUSE_SCROLL ${1:horizontal} ${2:vertical}"
        ],
        "description": "Scroll mouse wheel"
    },
    "Left Mouse Click": {
        "prefix": "LMOUSE",
        "body": ["LMOUSE"],
        "description": "Click Mouse Left Button"
    },
    "Right Mouse Click": {
        "prefix": "RMOUSE",
        "body": ["RMOUSE"],
        "description": "Click Mouse Right Button"
    },
    "Middle Mouse Click": {
        "prefix": "MMOUSE",
        "body": ["MMOUSE"],
        "description": "Click Mouse Middle Button"
    },
    "Forward Mouse Click": {
        "prefix": "FMOUSE",
        "body": ["FMOUSE"],
        "description": "Click Mouse Forward Button"
    },
    "Back Mouse Click": {
        "prefix": "BMOUSE",
        "body": ["BMOUSE"],
        "description": "Click Mouse Back Button"
    },
    "Loop Label": {
        "prefix": "LOOP",
        "body": [
            "LOOP${1:0}:",
            "\t${2:STRINGLN Action}"
        ],
        "description": "Assign multiple actions to one key (LOOP0, LOOP1, etc.)"
    },
    "Variable Declaration": {
        "prefix": "VAR",
        "body": [
            "VAR ${1:name} = ${2:0}"
        ],
        "description": "Declare variable"
    },
    "Define Constant": {
        "prefix": "DEFINE",
        "body": [
            "DEFINE ${1:NAME} ${2:value}"
        ],
        "description": "Define a constant to be replaced during preprocessing"
    },
    "If Statement": {
        "prefix": "IF",
        "body": [
            "IF ${1:condition}",
            "\t$0",
            "END_IF"
        ],
        "description": "Conditional statements"
    },
    "Else If": {
        "prefix": "ELSE IF",
        "body": [
            "ELSE IF ${1:condition}",
            "\t"
        ],
        "description": "Else If condition"
    },
    "Else": {
        "prefix": "ELSE",
        "body": [
            "ELSE",
            "\t"
        ],
        "description": "Else condition"
    },
    "While Loop": {
        "prefix": "WHILE",
        "body": [
            "WHILE ${1:condition}",
            "\t$0",
            "END_WHILE"
        ],
        "description": "Repeat actions while condition is true"
    },
    "Loop Continue": {
        "prefix": "CONTINUE",
        "body": ["CONTINUE"],
        "description": "Restart at beginning of loop"
    },
    "Loop Break": {
        "prefix": "LBREAK",
        "body": ["LBREAK"],
        "description": "Exit loop"
    },
    "Function": {
        "prefix": "FUN",
        "body": [
            "FUN ${1:name}(${2:args})",
            "\t$0",
            "END_FUN"
        ],
        "description": "Define a function"
    },
    "Return": {
        "prefix": "RETURN",
        "body": [
            "RETURN ${1:value}"
        ],
        "description": "Return from function (value optional)"
    },
    "Profile Switch Previous": {
        "prefix": "PREV_PROFILE",
        "body": ["PREV_PROFILE"],
        "description": "Go to previous profile"
    },
    "Profile Switch Next": {
        "prefix": "NEXT_PROFILE",
        "body": ["NEXT_PROFILE"],
        "description": "Go to next profile"
    },
    "Goto Profile": {
        "prefix": "GOTO_PROFILE",
        "body": [
            "GOTO_PROFILE ${1:ProfileName}"
        ],
        "description": "Jump to a profile by name (Case Sensitive)"
    },
    "OLED Print": {
        "prefix": "OLED_PRINT",
        "body": [
            "OLED_PRINT ${1:text}"
        ],
        "description": "Print message to display buffer"
    },
    "OLED Center Print": {
        "prefix": "OLED_CPRINT",
        "body": [
            "OLED_CPRINT ${1:text}"
        ],
        "description": "Print center-aligned message to display buffer"
    },
    "OLED Cursor": {
        "prefix": "OLED_CURSOR",
        "body": [
            "OLED_CURSOR ${1:x} ${2:y}"
        ],
        "description": "Set OLED cursor position"
    },
    "OLED Clear": {
        "prefix": "OLED_CLEAR",
        "body": ["OLED_CLEAR"],
        "description": "Clear the display buffer"
    },
    "OLED Update": {
        "prefix": "OLED_UPDATE",
        "body": ["OLED_UPDATE"],
        "description": "Push buffer to OLED display"
    },
    "OLED Restore": {
        "prefix": "OLED_RESTORE",
        "body": ["OLED_RESTORE"],
        "description": "Restore default display"
    },
    "OLED Circle": {
        "prefix": "OLED_CIRCLE",
        "body": [
            "OLED_CIRCLE ${1:x} ${2:y} ${3:radius} ${4:fill}"
        ],
        "description": "Draw circle (fill: 0 or 1)"
    },
    "OLED Line": {
        "prefix": "OLED_LINE",
        "body": [
            "OLED_LINE ${1:x1} ${2:y1} ${3:x2} ${4:y2}"
        ],
        "description": "Draw line"
    },
    "OLED Rect": {
        "prefix": "OLED_RECT",
        "body": [
            "OLED_RECT ${1:x1} ${2:y1} ${3:x2} ${4:y2} ${5:fill}"
        ],
        "description": "Draw rectangle (fill: 0 or 1)"
    },
    "RGB Set": {
        "prefix": "SWC_SET",
        "body": [
            "SWC_SET ${1:key_n} ${2:r} ${3:g} ${4:b}"
        ],
        "description": "Set LED color (n=0 for current)"
    },
    "RGB Fill": {
        "prefix": "SWC_FILL",
        "body": [
            "SWC_FILL ${1:r} ${2:g} ${3:b}"
        ],
        "description": "Set all LEDs to color"
    },
    "RGB Reset": {
        "prefix": "SWC_RESET",
        "body": [
            "SWC_RESET ${1:key_n}"
        ],
        "description": "Reset key LED color (0:current key, 1-20: particular key, 99:all keys)"
    },
    "Use StdLib": {
        "prefix": "USE_STDLIB",
        "body": ["USE_STDLIB"],
        "description": "Include duckyPad Standard Library"
    },
    "Use User Header": {
        "prefix": "USE_UH",
        "body": ["USE_UH"],
        "description": "Include User Header"
    },
    "Rand Int": {
        "prefix": "RANDINT",
        "body": [
            "RANDINT(${1:lower}, ${2:upper})"
        ],
        "description": "Get random integer between lower and upper (inclusive)"
    },
    "Rand Unsigned Int": {
        "prefix": "RANDUINT",
        "body": [
            "RANDUINT(${1:lower}, ${2:upper})"
        ],
        "description": "Get random unsigned integer between lower and upper (inclusive)"
    },
    "Sleep": {
        "prefix": "DP_SLEEP",
        "body": ["DP_SLEEP"],
        "description": "Put device to sleep"
    },
    "Halt": {
        "prefix": "HALT",
        "body": ["HALT"],
        "description": "Stop execution immediately"
    },
    "Clear Buffer": {
        "prefix": "BCLR",
        "body": ["BCLR"],
        "description": "Clear internal keypress event queue"
    },

    "Peek 8-bit": {
        "prefix": "PEEK8",
        "body": ["PEEK8(${1:addr})"],
        "description": "Read SIGNED 8-bit value at memory address"
    },
    "Peek 16-bit": {
        "prefix": "PEEK16",
        "body": ["PEEK16(${1:addr})"],
        "description": "Read SIGNED 16-bit value at memory address"
    },
    "Peek 32-bit": {
        "prefix": "PEEK32",
        "body": ["PEEK32(${1:addr})"],
        "description": "Read 32-bit value at memory address"
    },
    "Peek Unsigned 8-bit": {
        "prefix": "PEEKU8",
        "body": ["PEEKU8(${1:addr})"],
        "description": "Read UNSIGNED 8-bit value at memory address"
    },
    "Peek Unsigned 16-bit": {
        "prefix": "PEEKU16",
        "body": ["PEEKU16(${1:addr})"],
        "description": "Read UNSIGNED 16-bit value at memory address"
    },
    "Poke 8-bit": {
        "prefix": "POKE8",
        "body": ["POKE8(${1:addr}, ${2:value})"],
        "description": "Write 8-bit value to memory address"
    },
    "Poke 16-bit": {
        "prefix": "POKE16",
        "body": ["POKE16(${1:addr}, ${2:value})"],
        "description": "Write 16-bit value to memory address"
    },
    "Poke 32-bit": {
        "prefix": "POKE32",
        "body": ["POKE32(${1:addr}, ${2:value})"],
        "description": "Write 32-bit value to memory address"
    },

    "Unsigned Less Than": {
        "prefix": "ULT",
        "body": ["ULT(${1:lhs}, ${2:rhs})"],
        "description": "Unsigned Less Than comparison"
    },
    "Unsigned Less Than or Equal": {
        "prefix": "ULTE",
        "body": ["ULTE(${1:lhs}, ${2:rhs})"],
        "description": "Unsigned Less Than or Equal comparison"
    },
    "Unsigned Greater Than": {
        "prefix": "UGT",
        "body": ["UGT(${1:lhs}, ${2:rhs})"],
        "description": "Unsigned Greater Than comparison"
    },
    "Unsigned Greater Than or Equal": {
        "prefix": "UGTE",
        "body": ["UGTE(${1:lhs}, ${2:rhs})"],
        "description": "Unsigned Greater Than or Equal comparison"
    },
    "Unsigned Division": {
        "prefix": "UDIV",
        "body": ["UDIV(${1:val}, ${2:n})"],
        "description": "Unsigned integer division"
    },
    "Unsigned Modulo": {
        "prefix": "UMOD",
        "body": ["UMOD(${1:val}, ${2:n})"],
        "description": "Unsigned modulus"
    },
    "Logical Right Shift": {
        "prefix": "LSR",
        "body": ["LSR(${1:val}, ${2:n})"],
        "description": "Logical Right Shift (Zero-Extend)"
    },
    
